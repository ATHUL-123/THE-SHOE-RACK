                            <script>
                                function increaseQuantity() {

                                    const quantityElement = document.getElementById('quantityValue');
                                    const currentQuantity = parseInt(quantityElement.innerText, 10);
                                    const newQuantity = currentQuantity + 1;

                                    quantityElement.innerText = newQuantity;
                                    document.getElementById('selectedQuantity').value = newQuantity;
                                }

                                function decreaseQuantity() {

                                    const quantityElement = document.getElementById('quantityValue');
                                    const currentQuantity = parseInt(quantityElement.innerText, 10);
                                    const newQuantity = Math.max(currentQuantity - 1, 1); // Ensure quantity doesn't go below 1
                                    if (newQuantity == 0) {
                                        quantityElement.innerText = newQuantity + 1
                                        document.getElementById('selectedQuantity').value = newQuantity;
                                    } else {
                                        quantityElement.innerText = newQuantity;
                                        document.getElementById('selectedQuantity').value = newQuantity;
                                    }

                                }
                            </script>
