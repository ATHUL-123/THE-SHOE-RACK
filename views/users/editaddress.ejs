    <script>
        function validateForm() {
            var addressname = document.forms["enq"]["addressname"].value;
            var name = document.forms["enq"]["name"].value;
            var lname = document.forms["enq"]["lname"].value;
            var phone = document.forms["enq"]["phone"].value;
            var city = document.forms["enq"]["city"].value;
            var pincode = document.forms["enq"]["pincode"].value;
            var landmark = document.forms["enq"]["landmark"].value;
            var house = document.forms["enq"]["house"].value;
            var road = document.forms["enq"]["road"].value;
           
            
            if (addressname.trim() == "" || name.trim() == "" || lname.trim() == "" || phone.trim() == "" || city.trim() == "" || pincode.trim() == "" || landmark.trim() == "" || house.trim() == "" || road.trim() == "" ) {
                Swal.fire({
            title: 'All fields must be filled out',
            showClass: {
                popup: 'animate__animated animate__fadeInDown'
            },
            hideClass: {
                popup: 'animate__animated animate__fadeOutUp'
            }
         });
            
            return false;
            window.location.href = `/address-form`
            fetch(`/address-form`, {
                method: 'GET'
            })
            }
    
            // You can add additional custom validations if needed.
    
            return true;
        }
    </script>
