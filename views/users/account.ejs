                                                            <input required="" value="<%=user.name%>" class="form-control square" id="firstName" name="name" type="text">
                                                            <span id="nameError" class="error-message"></span>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label for="phone">Phone <span class="required">*</span></label>
                                                            <input required="" value="<%=user.phone%>" class="form-control square" id="phone" name="phone">
                                                            <span id="phoneError" class="error-message"></span>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label for="email">Email Address <span class="required">*</span></label>
                                                            <input required="" value="<%=user.email%>" class="form-control square" id="email" name="email" type="email">
                                                            <span id="emailError" class="error-message"></span>
                                                        </div>
                                                    </div>
                                                    <button type="submit" class="btn-brand ">Save Cahnges</button>
                                                </form>
                                                <script>
                                                    const form = document.getElementById('updateUserForm');
                                                
                                                    form.addEventListener('submit', function(event) {
                                                        let isValid = true;
                                                
                                                        const nameInput = document.getElementById('firstName').value.trim();
                                                        const phoneInput = document.getElementById('phone').value.trim();
                                                        const emailInput = document.getElementById('email').value.trim();
                                                
                                                        const nameError = document.getElementById('nameError');
                                                        const phoneError = document.getElementById('phoneError');
                                                        const emailError = document.getElementById('emailError');
                                                
                                                        // Name validation
                                                        if (nameInput === '') {
                                                            nameError.innerText = 'Please enter your name';
                                                            nameError.style.color = 'red';
                                                            isValid = false;
                                                        } else {
                                                            nameError.innerText = '';
                                                        }
                                                
                                                        // Phone number validation
                                                        if (phoneInput === '' || !isValidPhoneNumber(phoneInput)) {
                                                            phoneError.innerText = 'Please enter a valid phone number';
                                                            phoneError.style.color = 'red';
                                                            isValid = false;
                                                        } else {
                                                            phoneError.innerText = '';
                                                        }
                                                
                                                        // Email validation
                                                        if (emailInput === '' || !isValidEmail(emailInput)) {
                                                            emailError.innerText = 'Please enter a valid email address';
                                                            emailError.style.color = 'red';
                                                            isValid = false;
                                                        } else {
                                                            emailError.innerText = '';
                                                        }
                                                
                                                        if (!isValid) {
                                                            event.preventDefault(); // Prevent form submission if validation fails
                                                        }
                                                    });
                                                
                                                    function isValidPhoneNumber(phone) {
                                                        // You can implement more sophisticated phone number validation here
                                                        return /^\d{10}$/.test(phone); // Validates if phone number has exactly 10 digits
                                                    }
                                                
                                                    function isValidEmail(email) {
                                                        // You can implement more sophisticated email validation here
                                                        return /^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email); // Validates basic email format
                                                    }
                                                </script>
                                                
                                                

