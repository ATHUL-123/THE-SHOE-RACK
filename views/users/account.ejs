<script>
    document.addEventListener("DOMContentLoaded", function() {
        const editProfilePicBtn = document.getElementById("editProfilePic");
        const imageInputField = document.getElementById("imageInputField");
        const imageInput = document.getElementById("imageInput");
        const imagePreview = document.getElementById("imagePreview");

        editProfilePicBtn.addEventListener("click", function(event) {
            event.preventDefault(); // Prevent the default behavior of the anchor tag

            // Show/hide the image input field
            if (imageInputField.style.display === "none") {
                imageInputField.style.display = "block";
            } else {
                imageInputField.style.display = "none";
            }
        });

        // Function to handle image preview
        imageInput.addEventListener("change", function() {
            const file = imageInput.files[0];

            if (file) {
                const reader = new FileReader();

                reader.onload = function(event) {
                    const img = new Image();
                    img.src = event.target.result;
                    img.style.maxHeight = "200px"; // Set maximum height for the preview image
                    imagePreview.innerHTML = "";
                    imagePreview.appendChild(img);
                };

                reader.readAsDataURL(file);
            } else {
                imagePreview.innerHTML = "No image selected";
            }
        });
    });
