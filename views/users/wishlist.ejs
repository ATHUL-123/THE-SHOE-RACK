    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="index.html" rel="nofollow">Home</a>
                    <span></span> Shop
                    <span></span> Wishlist
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Stock Status</th>
                                        <th scope="col">Action</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (wishlist && wishlist.products.length > 0) { %>
                                    <%for(let i=0;i< wishlist.products.length;i++){%>
                                    <tr>
                                        <td class="image product-thumbnail"><img src="<%=wishlist.products[i].productId.images_url[0]%>" alt="#"></td>
                                        <td class="product-des product-name">
                                            <h5 class="product-name"><a href="shop-product-right.html"> <%=wishlist.products[i].productId.productName %></a></h5>
                                            <p class="font-xs">Brand : <%=wishlist.products[i].productId.Brand%><br><%=wishlist.products[i].productId.category.name%>
                                            </p>
                                        </td>
                                        
                                        <% const price = wishlist.products[i].productId.discountedPrice > 0
                                        ? wishlist.products[i].productId.discountedPrice
                                        : wishlist.products[i].productId.salePrice;
                                      %>
                                      
                                        <td class="price" data-title="Price"><span>₹<%=price%></span></td>
                                        <td class="text-center" data-title="Stock">
                                            <%if(wishlist.products[i].productId.is_active){%>
                                            <span class="color3 font-weight-bold">In Stock</span>
                                            <%}else{%>
                                                <span class="color3 font-weight-bold text-danger">Out Of Stock</span>
                                                    <%}%>
                                           
                                        </td>
                                        <td class="text-right" data-title="Cart">
                                            <%if(wishlist.products[i].productId.is_active){%>
                                            <a href="/productdetails/<%=wishlist.products[i].productId._id%>"><button class="btn btn-sm"><i class="fi-rs-shopping-bag mr-5"></i>Add to cart</button></a>
                                            <%}else{%>
                                                <a href="#"><button type="button" class="btn-sm text-white btn" style="background-color: rgb(9, 92, 120)"><i class="fi-rs-mobile mr-5"></i>Contact Us</button></a>
                                                <%}%>
                                        </td>
                                        <td class="action" data-title="Remove"><a href="/removeitem-wishlist?productId=<%=wishlist.products[i].productId%>&index=<%=i%>"><i class="fi-rs-trash"></i></a></td>
                                    </tr>
                                   
                                  <%}%>
                                </tbody>
                            </table>
                            <%}else{%>
                                    
                                <h1><span>Your Wishlist Is Empty!</span></h1>
                                <%}%>
                        </div>
                    </div>
                </div>
            </div>
