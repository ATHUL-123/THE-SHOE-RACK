<script>
const totalAmountValue = document.querySelector('input[name="totalAmount"]').value;
const couponCodeValue = document.querySelector('input[name="couponcode"]').value;
const discountValue = document.querySelector('input[name="discount"]').value;
const afterDiscountValue = document.querySelector('input[name="afterdiscount"]').value;

        let selectedAddressId = 0;
      
        function updateSelectedAddressId(addressId) {
          selectedAddressId = addressId;
        }
      
          document.addEventListener("DOMContentLoaded", function () {
          const requestOptions = {
            method: "POST",
            headers: {
              "Content-Type": "application/json"
            },
            body: JSON.stringify({
              amount: "50000"
            })
          };
      
          var rzp1;
      
          // Function to initiate payment
          function initiatePayment(orderId, paymentStatus) {
            let order = JSON.stringify(orderId);
            console.log(order);
            var options = {
              "key": "rzp_test_EkB64T3dRlXXxF",
              "amount":  Math.ceil(order.amount),
              "currency": "INR",
              "name": "The Shoe Rack",
              "description": "Test Transaction",
              "order_id": orderId.id,
              "handler": function (response) {
                window.location.href = `/onlinepayment?orderId=${orderId.id}&status=Success`;
              },
              "theme": {
                "color": "#3399cc"
              }
            };
      
            rzp1 = new Razorpay(options);
      
            rzp1.on('payment.failed', function (response) {
              console.log("fail");
            //   alert(response.error.code);
            //   alert(response.error.description);
            //   alert(response.error.source);
            //   alert(response.error.step);
            //   alert(response.error.reason);
            //   alert(response.error.metadata.order_id);
            //    alert(response.error.metadata.payment_id);
            //   window.location.href = `/onlinepayment?orderId=${orderId.id}&status=Failed`;
            });
      
            rzp1.open();
          }
       
          // Add a click event listener to the "Pay Now (Online)" button
          document.getElementById('rzp-button1').addEventListener('click', function () {
            // Make the fetch request to create a new orderId
            event.preventDefault();
            fetch(`/onlinepayment?addressId=${selectedAddressId}&totalAmount=${totalAmountValue}&couponcode=${couponCodeValue}&discount=${discountValue}&afterdiscount=${afterDiscountValue}`, requestOptions)
              .then(response => response.json())
              .then(data => {
                const orderId = data.razorOrder;
                const paymentStatus = data.paymentStatus; // Assuming you have paymentStatus in the response
                // Call the initiatePayment function with the orderId and selectedAddressId
                initiatePayment(orderId, paymentStatus);
              })
              .catch(error => {
                console.error("Fetch error:", error);
              });
          });
        });
      </script>
      
      
      
    

