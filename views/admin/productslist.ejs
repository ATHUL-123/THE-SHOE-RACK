        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                    
                </div>
                <div>
                    
                    <a href="/admin/addproducts" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="example">
                            <thead>
                                <tr>
                                    <th class="text-start">Product</th>
                                    <th class="text-sm-end ps-4">Category</th>
                                    <th class="text-sm-end ps-4">Price</th>
                                    <th class="text-sm-end">Status</th>
                                    <th class="text-end">Brand</th>
                                    <th class="text-end">stock</th>
                                    <th class="text-end">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if(products.length > 0) { %>
                                    <% for(let i = 0; i < products.length; i++) { %>
                                        <%products.forEach(product => {
                                            let totalQuantity = 0;
                                            product.sizes.forEach(size => {
                                                totalQuantity += size.quantity;
                                            });
                                            product.totalQuantity = totalQuantity;
                                        });%>
                                        <tr>
                                            <td>
                                                <div class="row align-items-center">
                                                    <div class="col-lg-4 col-sm-4 col-8 flex-grow-1 col-name">
                                                        <a class="itemside" href="#">
                                                            <div class="left">
                                                                <img src="/<%=products[i].images_url[0]%>" class="img-sm img-thumbnail" alt="Item">
                                                            </div>
                                                            <div class="info">
                                                                <h6 class="mb-0"><%=products[i].productName%></h6>
                                                            </div>
                                                        </a>
                                                    </div>
                                                </div>
                                            </td>
                                            <td class="text-sm-end ps-4"><%=products[i].category.name%></td>
                                            <td class="text-sm-end ps-4"><%=products[i].salePrice%></td>
                                            <td class="text-sm-end">
                                                <% if(products[i].is_active) { %>
                                                    <span class="badge rounded-pill alert-success">Active</span>
                                                <% } else { %>
                                                    <span class="badge rounded-pill alert-danger">Disabled</span>
                                                <% } %>
                                            </td>
                                            <td class="text-end"><%=products[i].Brand%></td>
                                            <td class="text-end"><%= products[i].totalQuantity %></td>
                                            <td class="text-end">
                                                <% if(products[i].is_active) { %>
                                                    <a href="productaction/<%=products[i]._id %>/0" class="btn btn-sm font-sm rounded btn-danger">
                                                        <i class="material-icons md-edit"></i>Unlist
                                                    </a>
                                                <% } else { %>
                                                    <a href="productaction/<%=products[i]._id %>/1" class="btn btn-sm font-sm rounded btn-success">
                                                        <i class="material-icons md-edit"></i>Addlist
                                                    </a>
                                                <% } %>
                                                <a href="editproduct/<%=products[i]._id%>" class="btn btn-sm font-sm btn-light rounded">Edit</a>
                                            </td>
                                        </tr>
                                    <% } %>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                     
                        <%if(message){%>
                               <script>
                                   document.addEventListener('DOMContentLoaded', function() {
                                       confirm();
                                   });
                                   </script>  
                            
                       <%}%>

