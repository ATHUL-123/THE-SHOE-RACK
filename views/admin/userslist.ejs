                </div>
            </div>      
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="example">
                            <thead>
                                <tr>
                                    <th>Users</th>
                                    <th>Email</th>
                                    <th>Status</th>
                                    <th class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if(users.length > 0) { %>
                                    <% for(let i = 0; i < users.length; i++) { %>
                                        <tr>
                                            <td width="40%">
                                                <a href="#" class="itemside">
                                                    <div class="left">
                                                        <%if(users[i].image){%>
                                                            <img src="/<%=users[i].image%>" class="img-sm img-avatar" alt="Userpic">
                                                            <%}else{%>
                                                        <img src="/assets/imgs/page/avatar-6.png" class="img-sm img-avatar" alt="Userpic">
                                                        <%}%>
                                                    </div>
                                                    <div class="info pl-3">
                                                        <h6 class="mb-0 title"><%= users[i].name %></h6>
                                                        <small class="text-muted"><%= users[i].phone %></small>
                                                    </div>
                                                </a>
                                            </td>
                                            <td><%= users[i].email %></td>
                                            <td>
                                               
                                                    <% if(users[i].is_active) { %>
                                        <span class="badge rounded-pill alert-success">Active</span>
                                                    <% } else { %>
                                        <span class="badge rounded-pill alert-danger">Inactive</span>
                                                    <% } %>
                                                
                                            </td>
                                            
                                            <td class="text-end">
                                                <%if(users[i].is_active){%>
                                                    
                                                    <button onclick="userUnlist('<%= users[i]._id %>')" value="0" class="btn btn-sm btn-danger rounded font-sm mt-15">Block</button>
                                                
                                                    <%}else{%>
                                                    
                                                        <button onclick="userAddList('<%= users[i]._id %>')"  value="1" class="btn btn-sm btn-success rounded font-sm mt-15">UnBlock</button>
                                                    <%}%>

                                                <a href="#" class="btn btn-sm btn-danger btn-brand rounded font-sm mt-15">View Details</a>
                                            
                                            </td>
                                        </tr>
                                    <% } %>
                                <% } %>
                            </tbody>
                            
