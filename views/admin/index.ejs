                <div class="col-xl-4 col-lg-12">
                    <div class="card mb-4">
                        <article class="card-body">
                            <h5 class="card-title">Brand Sale</h5>
                            <canvas id="Chart" height="200px"></canvas>
                        </article>
                    </div>
                </div>
            </div>
            <% let values = [0, 0, 0, 0, 0, 0]; %>
            <% for (let i = 0; i < order.length; i++) { %>
                <% for (let j = 0; j < order[i].products.length; j++) { %>
                    <% let currentBrand = order[i].products[j].productId.Brand; %>
                   
                    <% if (currentBrand === 'NIKE') { %>
                        <% values[0] += order[i].products[j].quantity; %>
                    <% } else if (currentBrand === 'ADIDAS') { %>
                        <% values[1] += order[i].products[j].quantity; %>
                    <% } else if (currentBrand === 'PUMA') { %>
                        <% values[2] += order[i].products[j].quantity; %>
                        
                    <% } else if (currentBrand === 'NEW BALANCE') { %>
                        <% values[3] += order[i].products[j].quantity; %>
                    <% } else { %>
                        <% values[4] += order[i].products[j].quantity; %>
                    <% } %>
                <% } %>
            <% } %>
            
            
            


        
            <script>
                var xValues = ["Nike", "Adidas", "Puma", "NewBalance", "Others"];
                var yValues = [<%=values.join(", ") %>];
                var barColors = ["#b91d47", "#00aba9", "#2b5797", "#e8c3b9", "#1e7145"];
        
                new Chart("Chart", {
                    type: "pie",
                    data: {
                        labels: xValues,
                        datasets: [{
                            backgroundColor: barColors,
                            data: yValues
                        }]
                    },
                    options: {
                        title: {
                            display: true,
                            text: "Brand wise Sales"
                        }
                    }
                });
            </script>
            <div class="card mb-4">
                <header class="card-header">
                    <h4 class="card-title">Latest orders</h4>
                   
                </header>
                <div class="card-body">
                    <div class="table-responsive">
                        <div class="table-responsive">
                            <table class="table align-middle table-nowrap mb-0" id="example">
                                <thead class="table-light">
                                    <tr>
                                       
                                        <th class="align-middle" scope="col">Order ID</th>
                                        <th class="align-middle" scope="col">Billing Name</th>
                                        <th class="align-middle" scope="col">Date</th>
                                        <th class="align-middle" scope="col">Total</th>
                                        <th class="align-middle" scope="col">Payment Status</th>
                                        <th class="align-middle" scope="col">Payment Method</th>
                                        <th class="align-middle" scope="col">Order Status</th>
                                        <th class="align-middle" scope="col">View Details</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%let n =order.length%>
                                    <%for(let i=n-1;i>=0;i--){%>
                                        <% const date = new Date(order[i].orderDate) %>

                                    <% const formattedDate = date.toLocaleDateString('en-GB', {
                                      day: '2-digit',
                                      month: '2-digit',
                                      year: 'numeric'
                                    }) %>
                                    <tr>
                                        
                                        <td><a href="#" class="fw-bold">#<%=order[i].orderID%></a> </td>
                                        <td><%=order[i].userId.name%></td>
                                        <td>
                                            <%=formattedDate%>
                                        </td>
                                        <td>
                                            â‚¹<%=order[i].totalAmount%>
                                        </td>
                                        <td>
                                           <%if(order[i].paymentStatus=='Pending'){%>
                                            <span class="badge badge-pill badge-soft-danger"><%=order[i].paymentStatus%></span>
                                            <%}else{%>
                                            <span class="badge badge-pill badge-soft-success"><%=order[i].paymentStatus%></span>
                                                <%}%>
                                        </td>
                                        <td>
                                            <i class="material-icons md-payment font-xxl text-muted mr-5"></i><%=order[i].PaymentMethod%>
                                        </td>
                                        <td>
                                            <%if(order[i].orderStatus=='Cancelled' || order[i].orderStatus=='Returned'){%>
                                                <span class="badge badge-pill badge-soft-danger"><%=order[i].orderStatus%></span>
                                                <%}else{%>
                                                <span class="badge badge-pill badge-soft-success"><%=order[i].orderStatus%></span>
                                                    <%}%>
                                           
                                        </td>
                                        <td>
                                            <a href="/admin/orderdetails?orderId=<%=order[i]._id%>" class="btn btn-xs"> View details</a>
                                        </td>
                                    </tr>
                                    <%}%>
                                    
                                </tbody>
                            </table>
                        </div>
                    </div> <!-- table-responsive end// -->
                </div>
            </div>
            
