                            <section class="content-body p-xl-4">
                               
                                <form action="" method="post" onsubmit="return validateForm()">
                                    <div class="row">
                                        <div class="col-lg-8">
                                            <div class="row gx-3">
                                                <div class="col-6 mb-3">
                                                    <label class="form-label">Name</label>
                                                    <input value="<%=admin.name%>" name="name" class="form-control" type="text" placeholder="Type here" required>
                                                </div>
                                                <div class="col-lg-6 mb-3">
                                                    <label class="form-label">Email</label>
                                                    <input value="<%=admin.email%>" name="email" id="email" class="form-control" type="email" placeholder="example@mail.com" required>
                                                    <small id="emailError" class="text-danger d-none">Invalid email format</small>
                                                </div>
                                                <div class="col-lg-6 mb-3">
                                                    <label class="form-label">Phone</label>
                                                    <input value="<%=admin.phone%>" name="phone" id="phone" class="form-control" type="tel" placeholder="+1234567890" required>
                                                    <small id="phoneError" class="text-danger d-none">Invalid phone number format</small>
                                                </div>
                                                <input type="text" hidden value="<%=admin._id%>" name="Id">
                                                <div class="col-lg-6 mb-3">
                                                    <label class="form-label">Birthday</label>
                                                    <input value="<%= admin.DOB %>" name="DOB" id="dob" class="form-control" type="date" required>
                                                    <small id="dobError" class="text-danger d-none">You must be 18 or older.</small>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br>
                                    <button class="btn btn-primary" type="submit">Save changes</button>
                                </form>
                                
                                <script>
                                    function validateForm() {
                                        const emailInput = document.getElementById('email');
                                        const phoneInput = document.getElementById('phone');
                                        const dobInput = document.getElementById('dob');
                                        const emailError = document.getElementById('emailError');
                                        const phoneError = document.getElementById('phoneError');
                                        const dobError = document.getElementById('dobError');
                                
                                        // Email validation
                                        const emailPattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                                        if (!emailPattern.test(emailInput.value)) {
                                            emailError.classList.remove('d-none');
                                        } else {
                                            emailError.classList.add('d-none');
                                        }
                                
                                        // Phone validation (basic pattern for demonstration)
                                        const phonePattern = /^\d{10}$/;

                                        if (!phonePattern.test(phoneInput.value)) {
                                            phoneError.classList.remove('d-none');
                                        } else {
                                            phoneError.classList.add('d-none');
                                        }
                                
                                        // DOB validation (age must be 18 or older)
                                        const dob = new Date(dobInput.value);
                                        const eighteenYearsAgo = new Date();
                                        eighteenYearsAgo.setFullYear(eighteenYearsAgo.getFullYear() - 18);
                                        if (dob > eighteenYearsAgo) {
                                            dobError.classList.remove('d-none');
                                        } else {
                                            dobError.classList.add('d-none');
                                        }
                                
                                        // Check if any error exists
                                        if (
                                            emailError.classList.contains('d-none') &&
                                            phoneError.classList.contains('d-none') &&
                                            dobError.classList.contains('d-none')
                                        ) {
                                            return true; // All fields are valid
                                        }
                                
                                        return false; // At least one field has an error
                                    }
                                </script>
                                
                                
                                
